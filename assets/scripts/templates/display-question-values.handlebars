<div class="category">
  <h3 class="category-title" id="{{this.id}}">{{category.name}}</h3>
  {{#each category.clues}}
    <ul>
      <li class="clue" id="{{this.id}}">
        <p class="value">{{value}}</p>
        <div class="question">
        </div>
        </p>
      </li>
    </ul>
  {{/each}}
  </div>
</div>
